  "scripts": {
    "start": "craco start",
    "dev": "craco start",
    "start:dev": "craco start",
    "build": "craco build",
    "test": "craco test --run-in-band --passWithNoTests --watchAll=false",
    "start:prod": "serve -s build -l 8082",
    "eject": "react-scripts eject",
    "routes:enable": "node scripts/enable-constant-routes.js",
    "routes:disable": "node scripts/disable-constant-routes.js",
    "routes:status": "node scripts/check-route-status.js",
    "permission:migrate": "node scripts/migrate-to-simplified-permissions.js",
    "permission:check": "node scripts/migrate-to-simplified-permissions.js",
    "test:e2e": "dotenv -e ../../.env.e2e -- playwright test --config=e2e/playwright.config.js",
    "test:e2e:ui": "dotenv -e ../../.env.e2e -- playwright test --config=e2e/playwright.config.js --ui",
    "test:e2e:debug": "dotenv -e ../../.env.e2e -- playwright test --config=e2e/playwright.config.js --debug",
    "test:e2e:headed": "dotenv -e ../../.env.e2e -- playwright test --config=e2e/playwright.config.js --headed",
    "test:e2e:report": "playwright show-report e2e/playwright-report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "tailwindcss": "^3.0.13"
  }
