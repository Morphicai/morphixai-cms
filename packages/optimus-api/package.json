  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:e2e": "dotenv -e .env.e2e -- cross-env NODE_ENV=e2e nest start",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "install:prod": "pnpm install --prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --config jest.unit.config.js --passWithNoTests --runInBand --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=e2e jest --config ./test/jest-e2e.json",
    "test:auth": "cross-env NODE_ENV=e2e jest --config ./test/jest-e2e.json --testPathPattern=auth",
    "test:oss": "cross-env NODE_ENV=e2e jest --config ./test/jest-e2e.json --testPathPattern=oss"
  },
